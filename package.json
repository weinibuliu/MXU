  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,json}\" \"*.{json,md}\"",
