    "@tauri-apps/plugin-http": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-process": "^2",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "i18next": "^25.7.4",
    "loglevel": "^1.9.2",
    "lucide-react": "^0.562.0",
    "marked": "^17.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^16.5.3",
    "semver": "^7.7.3",
